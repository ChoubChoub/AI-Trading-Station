          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "none",
            "showPoints": "auto",
            "spanNulls": false
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
